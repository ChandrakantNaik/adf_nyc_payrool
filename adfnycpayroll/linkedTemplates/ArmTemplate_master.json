{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfnycpayroll"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"ls_adls_adf_nycpayroll_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_adf_nycpayroll'"
		},
		"ls_adls_nycpayroll_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_nycpayroll'"
		},
		"ls_sqldb_nypayroll_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqldb_nypayroll'"
		},
		"ls_synapsesqlpool_nycpayroll_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_synapsesqlpool_nycpayroll'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnycpayroll.dfs.core.windows.net/"
		},
		"ls_adls_adf_nycpayroll_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://synapsenycpayrollacct.dfs.core.windows.net/"
		},
		"ls_adls_nycpayroll_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnycpayroll.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfnycpayroll_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"ls_adls_adf_nycpayroll_accountKey": {
						"value": "[parameters('ls_adls_adf_nycpayroll_accountKey')]"
					},
					"ls_adls_nycpayroll_accountKey": {
						"value": "[parameters('ls_adls_nycpayroll_accountKey')]"
					},
					"ls_sqldb_nypayroll_connectionString": {
						"value": "[parameters('ls_sqldb_nypayroll_connectionString')]"
					},
					"ls_synapsesqlpool_nycpayroll_connectionString": {
						"value": "[parameters('ls_synapsesqlpool_nycpayroll_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"ls_adls_adf_nycpayroll_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_adf_nycpayroll_properties_typeProperties_url')]"
					},
					"ls_adls_nycpayroll_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_nycpayroll_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}